{
  "files.exclude": {
    ".cache": true,
    "icon": true,
    "node_modules": true,
    "public": true,
    ".env": true,
    ".gitignore": true,
    "desktop.ini": true,
    "gatsby-browser.js": true,
    "gatsby-ssr.js": true,
    "gatsby-config.js": true,
    "gatsby-node.js": true,
    "package-lock.json": true,
    "package.json": true,
    "tsconfig.json": true,
    // "src/root.tsx": true,
    // "src/page.tsx": true,
    ".babelrc": true,
    "netlify.toml": true
  },
  "vsicons.associations.folders": [
    {
      "icon": "config",
      "extensions": ["react"]
    },
    {
      "icon": "component",
      "extensions": ["Component-groups"]
    }
  ],
  "cSpell.language": "nl, en"
}
